<p id="notice"><%= notice %></p>
<%= form_tag 'import', :multipart =>true do  %>
    <div id="import-csv">
      <%= label_tag "Select a csv file to upload" %>
      <%= file_field_tag :csv %>
    </div>

    <div id="submit">
      <%= submit_tag "start Upload", :disable_with => 'Uploading...' %>
    </div>
<%end%>